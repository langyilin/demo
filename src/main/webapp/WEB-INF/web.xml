<web-app>
  <display-name>Archetype Created Web Application</display-name>


  <servlet>
    <servlet-name>ctg</servlet-name>
    <servlet-class>cn.cloudtogo.ide.demo.servlet.CTGServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ctg</servlet-name>
